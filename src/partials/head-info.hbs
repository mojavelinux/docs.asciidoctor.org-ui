    {{#with page.canonicalUrl}}
    <link rel="canonical" href="{{{this}}}">
    {{/with}}
    {{#if page.component}}
    <meta name="page-spec" content="{{spec page}}">
    <meta name="docsearch:component_title" content="{{{page.component.title}}}">
    {{#if (eq page.version 'master')}}
    <meta name="docsearch:component_version" content='["{{{./page.component.name}}}","{{{./page.component.name}}}"]'>
    {{else}}
    {{#with page.componentVersion}}
    <meta name="docsearch:component_version" content='["{{{./name}}}@{{{./version}}}"{{#if (eq this @root.page.component.latest)}},"{{{./name}}}"{{/if}}]'>
    <meta name="docsearch:version" content="{{{./version}}}">
    <meta name="docsearch:display_version" content='"{{{./displayVersion}}}"'>
    {{/with}}
    {{/if}}
    {{/if}}
    {{#with page.description}}
    <meta name="description" content="{{this}}">
    {{/with}}
    {{#with page.keywords}}
    <meta name="keywords" content="{{this}}">
    {{/with}}
    {{#with (or antoraVersion site.antoraVersion)}}
    <meta name="generator" content="Antora {{this}}">
    {{/with}}
    {{#unless (eq page.attributes.pagination undefined)}}
    {{#with page.previous}}
    <link rel="prev" href="{{{relativize ./url}}}">
    {{/with}}
    {{#with page.next}}
    <link rel="next" href="{{{relativize ./url}}}">
    {{/with}}
    {{/unless}}
